event: extensions
op: and
rules:
  - op: is
    path: routing/hostname
    value: ext-secureannex
  - op: or
    rules:
      - op: contains
        path: event/results/extensions/?/categories/?/
        value: Remote Access
      - op: contains
        path: event/results/extensions/?/categories/?/
        value: Proxy Avoidance
